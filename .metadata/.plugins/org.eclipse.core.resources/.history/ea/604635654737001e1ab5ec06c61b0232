package array_test;

// 2차 배열

public class array6 {

	public static void main(String[] args) {
		new array6().test6();
	}
	public void test6() {
		String member[][] = {
				{"박초롬","윤보미","정은지","김남주","오하영"},
				{"31","29","33","31","32"}
		};
		int ea = member.length;
		int data_ea = member[0].length;
		int data2_ea = member[1].length;
		
		/*
		 * 응용문제 7  : 다음 코드를 활용하여 결과값을 예시처럼 출력 되도록 하시오.
		 * */
		
		int f,ff;
		for(f=0; f < ea; f++) {		// 2
			if(f == 0) {
				for(ff=0; ff<data_ea; ff++) {
					System.out.println(member[f][ff]);
				}
			}else if(f == 1) {
				for(ff=0; ff<data_ea; ff++) {
					System.out.println(member[f][ff]);
				}
			}
		}
		
	}
}
